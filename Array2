//Find the maximum and minimum element in an array


#include <iostream>
using namespace std;

int main() {
	    int n=7, max=0, min=0;
	    int a[n];
	    for (int i = 0; i < n; i++) {
	        /* code */
	        cin>>a[i];
	    }
	    max=a[0];
	    min=a[0];
	    
	    for (int i = 1; i <n; i++) 
	    {
	       if(a[i]<min)
	        min=a[i];
	       if(a[i]>max)
	        max=a[i];
	    }
	    cout<<"min="<<min;
	    cout<<endl<<"max="<<max;
}
